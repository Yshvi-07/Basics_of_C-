using System;
class HelloWorld {
   public static void Main() {
       HelloWorld hw = new HelloWorld();    //instance Method
       hw.numberTables();
      
  }
  public void numberTables(){
      for(int i =1;i<=10;i++){
          int s= 2*i;
          Console.WriteLine("2 * {0} = {1}" , i,s);
      }
  }
}
